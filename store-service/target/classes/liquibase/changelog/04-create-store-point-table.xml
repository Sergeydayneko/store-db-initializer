<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="sdayneko" id="pr-4">
        <preConditions>
            <not><tableExists tableName="store_point"/></not>
        </preConditions>
        <createTable schemaName="purchase_management" tableName="store_point">
            <column name="store_point_id" type="INTEGER" autoIncrement="true">
                <constraints
                    primaryKey="true"
                    primaryKeyName="store_point_fk"
                    unique="true"/>
            </column>
            <column name="store_point_name" type="VARCHAR(100)">
                <constraints nullable="false" unique="false" />
            </column>
            <column name="store_point_address" type="VARCHAR(255)"> <!-- Has to be diff table -->
                <constraints nullable="false" unique="true" />
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>